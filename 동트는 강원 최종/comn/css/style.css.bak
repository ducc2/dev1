.con_wrap .right_menu .hash_news .news_list { width:250px; border:1px solid #ddd; }
.con_wrap .right_menu .hash_news .news_list .hash_tag_wrap { padding: 20px 20px 0; width: 210px;}
.con_wrap .right_menu .hash_news .news_list .hash_tag_wrap span.hash_tag { margin-bottom: 7px; margin-right: 7px; float: left; width:54px; text-align:center; height:23px; line-height:23px; background:#10ba00; border-radius:2px;}
.con_wrap .right_menu .hash_news .news_list .hash_tag_wrap span.hash_tag a {color:#fff; font-size:12px;}
.con_wrap .right_menu .hash_news .news_list > ul { padding: 0px 20px 13px;}
.con_wrap .right_menu .hash_news .news_list > ul li { padding: 9px 0 9px; border-bottom:1px dotted #ddd;}
.con_wrap .right_menu .hash_news .news_list > ul li:last-child { border:none;}
.con_wrap .right_menu .hash_news .news_list .news_pic {float:left; margin-right: 10px;}
.con_wrap .right_menu .hash_news .news_list .txt {float:left; font-size:12px; line-height: 14px;display: -webkit-box;  text-overflow: ellipsis; -webkit-box-orient: vertical; -webkit-line-clamp: 2; overflow: hidden; width:58%;}
.con_wrap .right_menu .hash_news .news_list .txt h4 {color:#f64b00; font-weight:bold;}
.con_wrap .right_menu .facebook_news {width: 250px; border-right:1px solid #ddd; border-left:1px solid #ddd; overflow: hidden;}
.con_wrap .right_menu .blog_link {width: 250px; border-right:1px solid #ddd; border-left:1px solid #ddd; overflow: hidden;}

/* footwrap */
.con_wrap .footwrap {margin-top:44px; width:100%; float:left;}
.con_wrap .footwrap .foot {border:1px solid #ddd; padding-left:20px; line-height: 58px; height: 58px;}
.con_wrap .footwrap .foot span {display:inline-block; float:left;}
.con_wrap .footwrap .foot span:after {padding: 0 7px; content: "│"; display:inline-block;}
.con_wrap .footwrap .foot span.non:after {display:none;}
.con_wrap .footwrap .foot span a{color:#303030; }
.con_wrap .footwrap .foot span a:hover {text-decoration:underline;}
.con_wrap .footwrap .foot .tab_sns {float:right;}
.con_wrap .footwrap .foot .tab_sns img {float: left;}
.con_wrap .footwrap p {color:#666; font-size:12px; padding:10px 0;}

/* media screen */
@media screen and (max-width:1200px) {
.gnbwrap { width: 100%;}
.gnbwrap .gnb {width: 1100px;}
.gnbwrap .gnb .menu li a { margin: 0 33px;}
.gnbwrap .locationwrap .location { width: 1100px;}
.con_wrap {width: 1100px;}
.con_wrap .container { width: 800px;}
.con_wrap .container .contents img {width:100%;}
.comment fieldset.write textarea { width: 710px;}

.con_wrap .main_rig_menu { width: 193px;}
.con_wrap .main_rig_menu .facebook_news {width: 193px;}
}
