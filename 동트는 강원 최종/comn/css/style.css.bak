.con_wrap .main_rig_menu .ranking_wrap ul.ranking li { padding: 5px; }
.con_wrap .main_rig_menu .ranking_wrap ul.ranking li a {letter-spacing: -1px;  display: -webkit-box;  text-overflow: ellipsis; -webkit-box-orient: vertical; -webkit-line-clamp: 1; overflow: hidden; width:100%;}
.con_wrap .main_rig_menu .ranking_wrap ul.ranking li:hover {background:#f5f5f5;}
.con_wrap .main_rig_menu .ranking_wrap ul.ranking li span.num_ranking {margin-right: 8px; display:inline-block; width:20px; height:20px; line-height:20px; background:#f86c17; color:#fff; font-size:12px; border-radius:2px; font-family: gotham; text-align: center;}
