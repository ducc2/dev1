@charset "utf-8";

@import url('http://fonts.googleapis.com/earlyaccess/nanumgothic.css');
@import url('http://fonts.googleapis.com/earlyaccess/nanummyeongjo.css');

/* HTML Reset ============================================================= */
html, body, div, ul, ol, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, pre, form, p, blockquote, fieldset, input, 
abbr, article, aside, command, details, figcaption, figure, footer, header, hgroup, mark, meter, nav, 
output, progress, section, summary, time, table, th, td {margin: 0;padding: 0;font-size: 13px;font-family: 'Nanum Gothic', sans-serif;line-height:1.5em;color:#333;-webkit-text-size-adjust:none;}
body {background:#fff;}
table {border-collapse: collapse;border-spacing: 0;}
img {border:0;vertical-align:middle;}
a {color: #333;text-decoration: none;}
a:link, a:visited, a:active {text-decoration: none;}
a:hover {text-decoration: none;}
nav, ul {list-style: none outside none;}
hr{display:none}
form {margin:0;padding:0;border:none;}
fieldset {margin:0;padding:0;border:none;}
caption {position:absolute;top:0;left:-1000em;width:0;height:0;line-height:0; display:none;}
legend {position:absolute;top:0;left:-1000em;width:0;height:0;line-height:0; display:none;}

input {vertical-align:middle; border:none; font-size:12px;}
input.radio { width:13px; height:13px; vertical-align:middle}
select { vertical-align:middle; font-size:12px;}
textarea {border-color: #b6b6b6 #ddd #ddd #b6b6b6; border-style: solid; border-width: 1px; background-color:#fff; font-size:12px;}
input[type=text], input[type=password] {border:1px solid #b6b6b6; background-color:#fff; padding:2px; height:20px;}
input.none {border:none !important; border-color:#fff; padding:2px;}
input.bg {background:#f2f8ff;}

.clearfix:before, .clearfix:after {content: ""; visibility: hidden; display: block; height: 0; clear: both;}
.hidden {display:none;}

/* Icon */
.icon_set {display:inline-block; background:url(../img/icon_set.png) no-repeat; background-size: 200px 274px; vertical-align: middle;}
.icon_before {width: 19px; height:17px; background-position: 0px -20px;  padding-right: 10px; }
.icon_request {width: 17px; height: 18px; background-position: -34px -20px; padding-right: 11px;}
.icon_facebook_s{width: 22px; height: 21px; background-position: -4px -57px; padding-right: 5px;}
.icon_blog_s{width: 22px; height: 21px; background-position: -34px -57px;}
.icon_home {width: 19px; height: 15px; background-position: 0px 0px;}
.share_sns { width: 21px; height: 20px; background-position: 0px -96px; padding-right:25px;}
.print{ width: 21px; height: 20px; background-position: -44px -96px; padding-right:25px;}
.list { width: 21px; height: 20px; background-position: -89px -96px; padding-right:18px;}
.alert_sns_f { width: 8px; height: 20px; background-position: 0px -134px; padding-right: 17px; border-right: 1px solid #ccc;}
.alert_sns_t { width: 12px; height: 20px; background-position: -29px -134px; padding: 0 14px; border-right: 1px solid #ccc;}
.alert_sns_k { width: 12px; height: 20px;  background-position: -72px -134px;  padding-left: 11px;}
.sns_c { width: 38px; height: 31px; background-position: -132px -11px; vertical-align: sub;}
.sns_f { width: 38px; height: 37px; background-position: -132px -53px; vertical-align: sub;}
.sns_t{ width: 38px; height: 37px; background-position: -132px -109px; vertical-align: sub;}
.sns_k { width: 38px; height: 37px; background-position: -132px -164px; vertical-align: sub;}

/* MainVisual =============================================================*/
.main_visual { background: url(../img/bg_main.png) no-repeat left top; clear: both;  width: 1600px; margin: auto;}
.main_slider {position: relative; top: 0; left: 0; margin-top: 8px; margin-bottom: 45px; width: 1200px;  margin: 0 auto;}
.main_slider .slider {position: relative; top:0; left: 0;z-index: 50; height: 520px; overflow: hidden;}
.main_slider .slider img {width:100%;}
.main_slider .slider .slider_txt {background-color:rgba(25, 46, 6, 0.67); width:220px; padding:30px; right: 50px; top: 50px; position: absolute; }
.main_slider .slider .slider_txt .title {color:#fff; font-size:30px; font-family:nanummyeongjo; font-weight:bold; display:block;}
.main_slider .slider .slider_txt .title span {line-height: 21px; text-align: center; color: #b8ff30; border-bottom: 1px solid #b8ff30; display: block; font-family:nanumgothic; font-size: 13px;width: 44px; margin: 0 auto 5px;}
.main_slider .slider .slider_txt p {color:#fff; font-weight:bold; padding:25px 0 40px; border-bottom:1px solid #adb5a8;}
.main_slider .slider .slider_txt .view_bottom {color:#fff; margin-top:20px; background: url(../img/icon_search_on.png) right center no-repeat; }
.main_slider .slider .slider_txt .view_bottom:hover {background: url(../img/icon_search_hover.png) right center no-repeat }
.main_slider .slider .slider_txt .view_bottom .view_t {padding: 0 9px; line-height: 18px; height: 18px; color:#103d0c; border-radius:50px; background:#adb5a8; font-size:12px; display:inline-block;}
.main_slider .btn_prev { cursor: pointer; outline: 0; border: none;  display: block;  position: absolute; top: 222px; left: -89px; width: 50px; height: 59px; background: url(../img/allow_left.png) 10px center no-repeat;  text-indent: -999px; overflow: hidden; zoom: 1; z-index: 998; border:1px solid #DEDEDE;}
.main_slider .btn_prev:hover {background: url(../img/allow_left.png) 10px center no-repeat rgba(0, 0, 0, 0.63); border:1px solid #000;}
.main_slider .btn_next {cursor: pointer;outline: 0;  border: none; display: block;  position: absolute;  top: 222px; right: -89px;  width: 50px;  height: 59px;  background: url(../img/allow_right.png) 12px center no-repeat; text-indent: -999px; overflow: hidden; zoom: 1;  z-index: 998; list-style:none; border:1px solid #DEDEDE;}
.main_slider .btn_next:hover {background: url(../img/allow_right.png) 12px center no-repeat rgba(0, 0, 0, 0.63); border:1px solid #000;}
.main_slider #paging {top: 463px; position: absolute; text-align: center; list-style: none;  left: 50%;  z-index: 100; margin-left: -100px;}
.main_slider #paging li {display: inline;}
.main_slider #paging li a {display: inline-block; margin: 0 6px; text-indent: -9999px;overflow: hidden; zoom: 1; background:#fff; width:28px; height:4px;}
.main_slider #paging li.current a { background:#f64b00; width:28px; height:4px;}
.t_hidden {display:none;}

/* main_contents */
.con_wrap {width:1200px; margin:35px auto; 0 }
.con_wrap .container {width:900px; float:left;}
.con_wrap .container .contents .title h2.tit_main {color:#333; font-size: 25px; margin-bottom: 0px; height: auto; display: inline-block;}
.con_wrap .container .contents .title h2.tit_main:after {padding: 0 7px; content: "│"; font-size: 18px; color: #dadada;}
.con_wrap .container .contents .title span.main_date {color:#666; margin-right: 20px; font-size:16px;}
.con_wrap .container .contents .title .last_view {background-color:#f5f5f5; border-radius:50px; border:1px solid #d3d3d3; color:#666; font-weight:bold; font-size: 12px; padding: 1px 10px; display: inline-block;}
.con_wrap .container .contents ul {width:900px;}
.con_wrap .container .contents ul li {float:left; padding-right: 30px; width: 278px; position: relative; }
.con_wrap .container .contents ul li a .thumb {width:278px; height:178px; overflow:hidden; box-sizing: border-box;  border: 1px solid #ddd;}
.con_wrap .container .contents ul li a .thumb .dark {border: 3px solid #f64b00; box-sizing: border-box;  width:278px; background-color:rgba(0,0,0,0.5); position:absolute; left:0; right:0; top:0; bottom:0; display:none; height:178px;}
.con_wrap .container .contents ul li a .thumb .dark > img {position:absolute; left:50%; margin-left:-29px; top:56px; border: 0;}
.con_wrap .container .contents ul li a .thumb:hover .dark {display:block;}
.con_wrap .container .contents ul li a .thumb:hover .def{ -webkit-transform: scale(1.1); transform: scale(1.1); transition-duration: 0.3s; transition: outline-color .5s;}
.con_wrap .container .contents ul li:nth-child(3n) {padding-right:0;}
.con_wrap .container .contents ul li h4.tit_con { font-weight:bold; color:#333;  text-align: center; font-size:14px; line-height: 24px; margin: 7px auto 30px auto;  display: -webkit-box;  text-overflow: ellipsis; -webkit-box-orient: vertical; -webkit-line-clamp: 1; overflow: hidden; width:62%;}
.con_wrap .container .contents ul li h4.tit_con  span {color:#10ba00; border-bottom: 1px solid #10ba00; display: block; font-size: 13px; width: 44px; margin: 0 auto 5px;}

/* main_right_menu */
.con_wrap .main_rig_menu { margin-top: 53px; float: right; width: 250px;border:1px solid #ddd;}
.con_wrap .main_rig_menu .facebook_news {width: 250px; border-bottom:1px solid #ddd; overflow: hidden;}
.con_wrap .main_rig_menu .ranking_wrap {border-bottom:1px solid #ddd;}
